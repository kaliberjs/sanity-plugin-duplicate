{
  "name": "@kaliber/sanity-plugin-duplicate",
  "version": "0.0.1",
  "description": "Adds a (usable) duplicate to sanity",
  "main": "index.js",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "sanipack build",
    "watch": "sanipack build --watch",
    "verify": "sanipack verify",
    "prepublishOnly": "sanipack build && sanipack verify",
    "prepare": "[ -f scripts/addPeerDependencies.js ] && node scripts/addPeerDependencies.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaliberjs/sanity-plugin-duplicate.git"
  },
  "keywords": [
    "sanity",
    "sanity-plugin",
    "duplicate"
  ],
  "files": [
    "index.js",
    "src/**",
    "LICENSE",
    "readme.md",
    "package.json",
    "sanity.json",
    "config.dist.json"
  ],
  "private": false,
  "license": "MIT",
  "author": "Kaliber Machinery <machinery@kaliber.net>",
  "devDependencies": {
    "sanipack": "^2.1.0"
  },
  "peerDependencies": {
    "@sanity/icons": "^1.3.1"
  }
}
